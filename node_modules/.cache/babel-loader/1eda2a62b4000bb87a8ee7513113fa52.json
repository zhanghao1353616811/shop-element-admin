f=\"formData\" :model=\"formData\" :rules=\"rules\" hide-required-asterisk>\r\n    <div class=\"login-name\">{{$config.logo}}</div>\r\n    <el-form-item class=\"form-item\" label=\"账号\" prop=\"userName\">\r\n      <el-input class=\"form-ipt\" v-model=\"formData.userName\" placeholder=\"请输入账号\" clearable></el-input>\r\n    </el-form-item>\r\n    <el-form-item class=\"form-item\" label=\"密码\" prop=\"password\">\r\n      <el-input class=\"form-ipt\" v-model=\"formData.password\" placeholder=\"请输入密码\" clearable></el-input>\r\n    </el-form-item>\r\n    <el-form-item>\r\n      <el-button class=\"form-item login-btn\" type=\"primary\" @click=\"submitForm('formData')\">立即登录</el-button>\r\n    </el-form-item>\r\n  </el-form>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      formData: {\r\n        userName: \"\",\r\n        password: \"\",\r\n      },\r\n      rules: {\r\n        userName: [\r\n          {\r\n            required: true,\r\n            message: \"请输入账号\",\r\n            trigger: \"blur\",\r\n          },\r\n          {\r\n            min: 6,\r\n            max: 16,\r\n            message: \"长度在6到16个字符\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        password: [\r\n          {\r\n            required: true,\r\n            message: \"请输入密码\",\r\n            trigger: \"blur\",\r\n          },\r\n          {\r\n            min: 6,\r\n            max: 16,\r\n            message: \"长度在6到16个字符\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm(formData) {\r\n      console.log(\r\n        this.$refs[formData].validate((valid) => {\r\n          if (valid) {\r\n            this.$router.push(\"index/index\");\r\n            this.$message(\"登录成功\");\r\n          } else {\r\n            this.$message(\"登录失败\");\r\n            return false;\r\n          }\r\n        })\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.form-Login {\r\n  position: absolute;\r\n  top: 35%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.login-name {\r\n  height: 60px;\r\n  font-size: 28px;\r\n  text-align: center;\r\n  line-height: 60px;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n.form-item {\r\n  width: 400px;\r\n  margin: 20px;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.form-ipt {\r\n  width: 340px;\r\n}\r\n\r\n.login-btn {\r\n  width: 390px;\r\n  margin: 0 auto;\r\n}\r\n</style>"],"sourceRoot":"src/views/login"}]}