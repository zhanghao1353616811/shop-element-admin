AAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAzBA;AA0BA,KArKA;AAsKA;AACA,IAAA,WAvKA,uBAuKA,IAvKA,EAuKA;AAAA;;AACA;AACA,UAAA,CAAA,IAAA,CAAA,OAAA,EAAA;AACA;AACA,aAAA,aAAA,CAAA,IAAA,CAAA;AACA,UAAA,EAAA,EAAA,IAAA,CAAA,EADA;AAEA,UAAA,GAAA,EAAA,IAAA,CAAA;AAFA,SAAA,EAFA,CAMA;;AACA,QAAA,IAAA,CAAA,UAAA,GAAA,KAAA,aAAA,CAAA,MAAA,CAPA,CAQA;;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,IAAA;AACA,eAVA,CAUA;AACA,OAbA,CAcA;;;AACA,UAAA,KAAA,GAAA,KAAA,aAAA,CAAA,SAAA,CAAA,UAAA,GAAA;AAAA,eAAA,GAAA,CAAA,EAAA,KAAA,IAAA,CAAA,EAAA;AAAA,OAAA,CAAA;AACA,UAAA,KAAA,KAAA,CAAA,CAAA,EAAA;AACA,UAAA,MAAA,GAAA,KAAA,aAAA,CAAA,MAAA,CAjBA,CAkBA;;AACA,UAAA,KAAA,GAAA,CAAA,GAAA,MAAA,EAAA;AAAA,mCACA,CADA;AAEA,cAAA,CAAA,GAAA,MAAA,CAAA,OAAA,CAAA,SAAA,CACA,UAAA,GAAA;AAAA,mBAAA,GAAA,CAAA,EAAA,KAAA,MAAA,CAAA,aAAA,CAAA,CAAA,EAAA,EAAA;AAAA,WADA,CAAA;;AAGA,cAAA,CAAA,GAAA,CAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,CAAA,EAAA,UAAA;AACA;AAPA;;AACA,aAAA,IAAA,CAAA,GAAA,KAAA,EAAA,CAAA,GAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AAAA,gBAAA,CAAA;AAOA;AACA,OA5BA,CA6BA;;;AACA,WAAA,aAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA,EA9BA,CA+BA;;AACA,MAAA,IAAA,CAAA,OAAA,GAAA,KAAA,CAhCA,CAiCA;;AACA,WAAA,UAAA,GAAA,CAAA;AACA,KA1MA;AA2MA;AACA,IAAA,YA5MA,0BA4MA;AAAA;;AACA,WAAA,OAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA;AACA,YAAA,KAAA,GAAA,MAAA,CAAA,aAAA,CAAA,SAAA,CAAA,UAAA,CAAA,EAAA;AACA,iBAAA,CAAA,CAAA,EAAA,KAAA,IAAA,CAAA,EAAA;AACA,SAFA,CAAA;;AAGA,YAAA,KAAA,GAAA,CAAA,CAAA,EAAA;AACA;AACA,UAAA,IAAA,CAAA,OAAA,GAAA,KAAA;AACA,UAAA,IAAA,CAAA,UAAA,GAAA,CAAA,CAHA,CAIA;;AACA,UAAA,MAAA,CAAA,aAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AACA,OAZA;AAaA,KA1NA;AA2NA,IAAA,gBA3NA,4BA2NA,GA3NA,EA2NA;AACA,MAAA,OAAA,CAAA,GAAA,wBAAA,GAAA;AACA,KA7NA;AA8NA,IAAA,mBA9NA,+BA8NA,GA9NA,EA8NA;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,GAAA;AACA;AAhOA;AAvCA,CAAA","sourcesContent":["<template>\r\n  <el-container class=\"image-warp\">\r\n    <el-header class=\"header-content\">\r\n      <div>\r\n        <el-select class=\"header-select\" v-model=\"search.order\" size=\"mini\" placeholder=\"输入图片排序方式\">\r\n          <el-option label=\"降序\" value=\"desc\"></el-option>\r\n          <el-option label=\"升序\" value=\"asce\"></el-option>\r\n        </el-select>\r\n        <el-input class=\"header-ipt\" v-model=\"search.kewords\" placeholder=\"输入图片名称\" size=\"mini\" clearable></el-input>\r\n        <el-button type=\"success\" size=\"mini\">搜索</el-button>\r\n      </div>\r\n      <div>\r\n        <el-button type=\"info\" size=\"mini\" v-if=\"chooseImgList.length\" @click=\"cancelChoose\">取消选中</el-button>\r\n        <el-button type=\"danger\" size=\"mini\" v-if=\"chooseImgList.length\" @click=\"delImg({all:true})\">批量删除</el-button>\r\n        <el-button type=\"success\" size=\"mini\" @click.stop=\"creOrUpPhotos(false)\">创建相册</el-button>\r\n        <el-button type=\"warning\" size=\"mini\" @click.stop=\"uploadImage()\">上传图片</el-button>\r\n      </div>\r\n    </el-header>\r\n    <el-container class=\"main-container\">\r\n      <el-aside class=\"aside-content\" width=\"200px\">\r\n        <photo-item class=\"aside-li\" v-for=\"(item, index) in photos\" :key=\"index\" :item=\"item\" :index=\"index\" :isActive=\"activeIndex === index\" @click=\"clickPhotosItem\" @update=\"creOrUpPhotos\" @del=\"delPhotos\">\r\n        </photo-item>\r\n      </el-aside>\r\n      <el-main class=\"main-content\">\r\n        <div class=\"flex-width\">\r\n          <el-card :class=\"[item.isCheck ? 'active-img' : '', 'img-flex']\" :body-style=\"{ padding: '0px' }\" shadow=\"hover\" v-for=\"(item, index) in imgList\" :key=\"index\">\r\n            <img class=\"img-src\" :src=\"item.src\" @click=\"chooseImage(item)\" />\r\n            <el-tag class=\"tag-tips\" type=\"danger\" size=\"mini\" v-if=\"item.isCheck\">{{ item.checkOrder }}</el-tag>\r\n            <div class=\"img-name\">{{ item.name }}</div>\r\n            <el-button-group class=\"image-btn\">\r\n              <el-button size=\"mini\" icon=\"el-icon-view\" @click=\"preItemImg(item)\"></el-button>\r\n              <el-button size=\"mini\" icon=\"el-icon-edit\" @click=\"openImgName(item)\"></el-button>\r\n              <el-button size=\"mini\" icon=\"el-icon-delete\" @click=\"delImg({index})\"></el-button>\r\n            </el-button-group>\r\n          </el-card>\r\n        </div>\r\n      </el-main>\r\n    </el-container>\r\n    <el-footer class=\"footer-content\">\r\n      <div  class=\"foot-btn\">\r\n        <el-button-group>\r\n          <el-button size=\"mini\" icon=\"el-icon-top\">上一页</el-button>\r\n          <el-button size=\"mini\" icon=\"el-icon-bottom\">下一页</el-button>\r\n        </el-button-group>\r\n      </div>\r\n      <div class=\"foot-pag\">\r\n        <el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" :current-page=\"currentPage\" :page-sizes=\"[100,200,300,400]\" :page-size=\"100\" layout=\"total, sizes, prev, pager, next, jumper\" :total=\"400\"></el-pagination>\r\n      </div>\r\n    </el-footer>\r\n    <!-- 修改创建相册 -->\r\n    <el-dialog width=\"700px\" :title=\"photoTitle\" :visible.sync=\"dialogFormVisible\" :close-on-click-modal=\"false\">\r\n      <el-form :model=\"photoData\">\r\n        <el-form-item label=\"相册名称\">\r\n          <el-input class=\"ipt-width\" v-model=\"photoData.name\" placeholder=\"请输入相册名称\" clearable></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"相册排序\">\r\n          <el-input-number v-model=\"photoData.order\" :min=\"0\"></el-input-number>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\">\r\n        <el-button type=\"info\" @click=\"dialogFormVisible = false\">取消</el-button>\r\n        <el-button type=\"success\" @click.stop.native=\"confirmPhoto\">确定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 上传图片 -->\r\n    <el-dialog width=\"700px\" title=\"上传图片\" :visible.sync=\"diaUpVisible\" :close-on-click-modal=\"false\">\r\n      <el-upload class=\"upload-img\" drag multiple action=\"https://jsonplaceholder.typicode.com/posts/\">\r\n        <i class=\"el-icon-upload\"></i>\r\n        <div>将文件拖到止处, 或<em>点击上传</em></div>\r\n        <div slot=\"tip\">只能上传jpg/png文件, 且不超过500kb</div>\r\n      </el-upload>\r\n    </el-dialog>\r\n    <!-- 预览图片 -->\r\n    <el-dialog :visible.sync=\"diaPreVisible\" :close-on-click-modal=\"false\">\r\n      <div class=\"pre-img\">\r\n        <img class=\"img-src\" :src=\"preImgSrc\" />\r\n      </div>\r\n    </el-dialog>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\nimport photoItem from \"@/components/image/photo-item.vue\";\r\nexport default {\r\n  components: {\r\n    photoItem,\r\n  },\r\n  data() {\r\n    return {\r\n      selectVal: \"降序\",\r\n      activeIndex: 0,\r\n      photos: [],\r\n      dialogFormVisible: false,\r\n      isUpdateIndex: -1, // 修改相册\r\n      diaUpVisible: false,\r\n      diaPreVisible: false,\r\n      preImgSrc: \"\",\r\n      imgList: [],\r\n      chooseImgList: [],\r\n      currentPage: 1,\r\n      search: [\r\n        {\r\n          order: \"desc\",\r\n          kewords: \"\",\r\n        },\r\n      ],\r\n      photoData: {\r\n        name: \"\",\r\n        order: \"\",\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    photoTitle() {\r\n      return this.isUpdateIndex > -1 ? \"修改相册\" : \"创建相册\";\r\n    },\r\n  },\r\n  created() {\r\n    this.initPhotos();\r\n    this.initImgList();\r\n  },\r\n\r\n  methods: {\r\n    initPhotos() {\r\n      for (let i = 0; i < 7; i++) {\r\n        this.photos.push({\r\n          name: \"相册\" + i,\r\n          num: Math.floor(Math.random() * 100),\r\n          order: 0,\r\n        });\r\n      }\r\n    },\r\n\r\n    initImgList() {\r\n      for (let i = 0; i < 30; i++) {\r\n        this.imgList.push({\r\n          id: i,\r\n          name: \"图片\" + i,\r\n          src: \"http://tangzhe123-com.oss-cn-shenzhen.aliyuncs.com/public/60c355f33ea91.jpg\",\r\n          isCheck: false,\r\n          checkOrder: 0, // 序号\r\n        });\r\n      }\r\n    },\r\n\r\n    clickPhotosItem(index) {\r\n      this.activeIndex = index;\r\n    },\r\n\r\n    creOrUpPhotos(Obj) {\r\n      // 修改相册\r\n      if (Obj) {\r\n        let { item, index } = Obj;\r\n        // 初始化相册\r\n        this.photoData.name = item.name;\r\n        this.photoData.order = item.order;\r\n        this.isUpdateIndex = index;\r\n        return (this.dialogFormVisible = true);\r\n      }\r\n      // 创建相册\r\n      this.photoData = {\r\n        name: \"\",\r\n        order: 0,\r\n      };\r\n      this.isUpdateIndex = -1;\r\n      this.dialogFormVisible = true;\r\n    },\r\n\r\n    confirmPhoto() {\r\n      if (this.photoData.name === \"\" || this.photoData.order === \"\") {\r\n        return (this.dialogFormVisible = false);\r\n      }\r\n      if (this.isUpdateIndex > -1) {\r\n        this.updatePhotos();\r\n        return (this.dialogFormVisible = false);\r\n      }\r\n      let photos = this.photos;\r\n      // 创建相册 => 追加到photos数组前\r\n      photos.unshift({\r\n        name: this.photoData.name,\r\n        order: this.photoData.order,\r\n        num: 0, // 第一次创建相册数量为0\r\n      });\r\n      // 数组去重\r\n      function ArrSet(arr, key) {\r\n        let newObj = {};\r\n        let newArr = [];\r\n        for (let i = 0; i < arr.length; i++) {\r\n          const item = arr[i];\r\n          if (!newObj[item[key]]) {\r\n            newObj[item[key]] = newArr.push(item);\r\n          }\r\n        }\r\n        return newArr;\r\n      }\r\n      let setArry = ArrSet(photos, \"name\");\r\n      this.photos = setArry;\r\n      this.dialogFormVisible = false;\r\n    },\r\n    // 修改相册\r\n    updatePhotos() {\r\n      this.photos[this.isUpdateIndex].name = this.photoData.name;\r\n      this.photos[this.isUpdateIndex].order = this.photoData.order;\r\n    },\r\n    // 删除相册\r\n    delPhotos(index) {\r\n      this.$confirm(\"是否确定删除相册\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n        closeOnClickModal: false, // 是否可通过点击遮罩层关闭 MessageBox\r\n      })\r\n        .then(() => {\r\n          this.photos.splice(index, 1);\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\",\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    uploadImage() {\r\n      this.diaUpVisible = true;\r\n    },\r\n    preItemImg(Obj) {\r\n      this.diaPreVisible = true;\r\n      this.preImgSrc = Obj.src;\r\n    },\r\n    openImgName(Obj) {\r\n      this.$prompt(\"请输入新名称\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        inputValue: Obj.name,\r\n        closeOnClickModal: false,\r\n        inputValidator(val) {\r\n          if (val === \"\") {\r\n            return \"图片名称不能为空\";\r\n          }\r\n        },\r\n      })\r\n        .then(({ value }) => {\r\n          Obj.name = value;\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"修改成功\",\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"取消输入\",\r\n          });\r\n        });\r\n    },\r\n    // 删除图片\r\n    delImg(Obj) {\r\n      this.$confirm(Obj.all ? \"是否删除选中图片?\" : \"是否删除该图片?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          if (Obj.all) {\r\n            let list = this.imgList.filter((item) => {\r\n              return !this.chooseImgList.some((v) => v.id === item.id);\r\n            });\r\n            this.imgList = list;\r\n            this.chooseImgList = [];\r\n          } else {\r\n            this.imgList.splice(Obj.index, 1);\r\n          }\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\",\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"取消删除\",\r\n          });\r\n        });\r\n    },\r\n    // 选中图片\r\n    chooseImage(item) {\r\n      // 选中\r\n      if (!item.isCheck) {\r\n        // 加入选中\r\n        this.chooseImgList.push({\r\n          id: item.id,\r\n          src: item.src,\r\n        });\r\n        // 计算序号\r\n        item.checkOrder = this.chooseImgList.length;\r\n        // 修改状态\r\n        item.isCheck = true;\r\n        return; // 若还做另一种判断则使用 return 中断\r\n      }\r\n      // 取消单个选中\r\n      let index = this.chooseImgList.findIndex((img) => img.id === item.id);\r\n      if (index === -1) return;\r\n      let length = this.chooseImgList.length;\r\n      // 删除最后一个的前面任意一个 => 后面的重新计算imgList选中序号\r\n      if (index + 1 < length) {\r\n        for (let j = index; j < length; j++) {\r\n          let i = this.imgList.findIndex(\r\n            (img) => img.id === this.chooseImgList[j].id\r\n          );\r\n          if (i > -1) {\r\n            this.imgList[i].checkOrder--;\r\n          }\r\n        }\r\n      }\r\n      // 找到chooseImgList的索引 删除\r\n      this.chooseImgList.splice(index, 1);\r\n      // 修改状态\r\n      item.isCheck = false;\r\n      // 重置序号\r\n      this.checkOrder = 0;\r\n    },\r\n    // 取消所有选中\r\n    cancelChoose() {\r\n      this.imgList.forEach((item) => {\r\n        // 找到所有选中的图片\r\n        let index = this.chooseImgList.findIndex((v) => {\r\n          return v.id === item.id;\r\n        });\r\n        if (index > -1) {\r\n          // 取消选中样式 选中排序归零\r\n          item.isCheck = false;\r\n          item.checkOrder = 0;\r\n          // 从chooseList中移除\r\n          this.chooseImgList.splice(index, 1);\r\n        }\r\n      });\r\n    },\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n    },\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.image-warp {\r\n  position: absolute;\r\n  top: 55px;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.header-content {\r\n  line-height: 60px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n.header-select {\r\n  width: 150px;\r\n}\r\n\r\n.header-ipt {\r\n  margin: 0 10px;\r\n  width: 200px;\r\n}\r\n\r\n.main-container {\r\n  position: relative;\r\n}\r\n\r\n.aside-content {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  background-color: #fff;\r\n  border-right: 1px solid #ccc;\r\n}\r\n\r\n.aside-li {\r\n  padding: 20px 0;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-around;\r\n  cursor: pointer;\r\n  border-bottom: 1px solid #ccc;\r\n}\r\n\r\n.drop-down {\r\n  width: 50px;\r\n  height: 25px;\r\n  line-height: 25px;\r\n  border-radius: 5px;\r\n  text-align: center;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.main-content {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 200px;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n\r\n.flex-width {\r\n  width: 100%;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  margin: 0 0 -5px 18px;\r\n}\r\n\r\n.img-flex {\r\n  width: 165px; /* 换行则子元素为固定像素 */\r\n  margin: 0 10px 5px 0px;\r\n  box-sizing: border-box;\r\n  cursor: pointer;\r\n  position: relative;\r\n}\r\n\r\n.active-img {\r\n  border: 1px solid #dc3545;\r\n}\r\n\r\n.img-src {\r\n  width: 100%;\r\n  height: 90px;\r\n  display: block;\r\n}\r\n\r\n.tag-tips {\r\n  position: absolute;\r\n  right: 0;\r\n  top: 0;\r\n}\r\n\r\n.img-name {\r\n  color: #fff;\r\n  text-align: center;\r\n  opacity: 0.8;\r\n  background: rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.image-btn {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin: 7px 0;\r\n}\r\n\r\n.footer-content {\r\n  display: flex;\r\n  align-items: center;\r\n  line-height: 60px;\r\n  border-top: 1px solid #ccc;\r\n}\r\n\r\n.foot-btn {\r\n  width: 185px;\r\n  margin-left: -6px;\r\n  border-right: 1px solid #ccc;\r\n}\r\n\r\n.foot-pag {\r\n  line-height: 0px;\r\n  margin-left: 34px;\r\n}\r\n\r\n.ipt-width {\r\n  width: 300px;\r\n}\r\n\r\n.upload-img {\r\n  text-align: center;\r\n}\r\n\r\n.pre-img {\r\n  margin: -60px -20px -30px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/image"}]}