!-- <el-form-item label=\"规格设置\"></el-form-item> -->\r\n        </el-form>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"商品属性\">商品属性</el-tab-pane>\r\n      <el-tab-pane label=\"媒体设置\">媒体设置</el-tab-pane>\r\n      <el-tab-pane label=\"商品详情\">商品详情</el-tab-pane>\r\n      <el-tab-pane label=\"折扣设置\">折扣设置</el-tab-pane>\r\n    </el-tabs>\r\n    <div class=\"btn-fixed\">\r\n      <el-button type=\"success\">保存</el-button>\r\n      <el-button class=\"btn-cancel\" type=\"info\">取消</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      activeIndex: \"0\",\r\n      sku: \"\",\r\n      radio: '',\r\n      formData: {\r\n        name: \"\",\r\n        category: [],\r\n        desc: \"\",\r\n        unit: \"\",\r\n        stock: \"\",\r\n        warn: \"\",\r\n        show: \"0\",\r\n        status: \"0\",\r\n        templ: \"\",\r\n      },\r\n      options: [\r\n        {\r\n          value: \"zhinan\",\r\n          label: \"指南\",\r\n          children: [\r\n            {\r\n              value: \"shejiyuanze\",\r\n              label: \"设计原则\",\r\n              children: [\r\n                {\r\n                  value: \"yizhi\",\r\n                  label: \"一致\",\r\n                },\r\n                {\r\n                  value: \"fankui\",\r\n                  label: \"反馈\",\r\n                },\r\n              ],\r\n            },\r\n            {\r\n              value: \"daohang\",\r\n              label: \"导航\",\r\n              children: [\r\n                {\r\n                  value: \"cexiangdaohang\",\r\n                  label: \"侧向航\",\r\n                },\r\n              ],\r\n            },\r\n          ],\r\n        },\r\n      ],\r\n      temOptions: [\r\n        {\r\n          value: \"选项1\",\r\n          label: \"区域1\",\r\n        },\r\n        {\r\n          value: \"选项2\",\r\n          label: \"区域2\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n};\r\n</script>\r