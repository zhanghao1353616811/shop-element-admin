el-cascader>\r\n          </el-form-item>\r\n          <el-form-item label=\"商品描述\">\r\n            <el-input class=\"form-input\" type=\"textarea\" v-model=\"formData.desc\" placeholder=\"选填，商品卖点简述，例如：此款商品美观大方 性价比较高 不容错过\" :rows=\"3\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"商品单位\">\r\n            <el-input class=\"form-input\" v-model=\"formData.unit\" placeholder=\"请输入商品单位\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"总库存\">\r\n            <el-input class=\"form-input\" type=\"number\" v-model=\"formData.stock\" placeholder=\"总库存\">\r\n              <template slot=\"append\">件</template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"库存预警\">\r\n            <el-input class=\"form-input\" type=\"number\" v-model=\"formData.warn\" placeholder=\"库存预警\">\r\n              <template slot=\"append\">件</template>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"库存显示\">\r\n            <el-radio-group v-model=\"formData.show\">\r\n              <el-radio label=\"1\" border>是</el-radio>\r\n              <el-radio label=\"0\" border>否</el-radio>\r\n            </el-radio-group>\r\n          </el-form-item>\r\n          <el-form-item label=\"是否上架\">\r\n            <el-radio-group v-model=\"formData.status\">\r\n              <el-radio label=\"1\" border>立刻上架</el-radio>\r\n              <el-radio label=\"0\" border>放入仓库</el-radio>\r\n            </el-radio-group>\r\n          </el-form-item>\r\n          <el-form-item label=\"运费模板\">\r\n            <el-select v-model=\"formData.templ\">\r\n              <el-option v-for=\"(item, index) in temOptions\" :key=\"index\" :label=\"item.label\" :value=\"item.value\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-tab-pane>\r\n      <el-tab-pane label=\"商品规格\">\r\n        <el-form label-width=\"70px\">\r\n          <el-form-item